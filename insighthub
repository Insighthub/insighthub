local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
    local Window = Library.CreateLib("Insight 0.1V", "BloodTheme")
 
    -- MAIN
    local Main = Window:NewTab("Main")
    local MainSection = Main:NewSection("Main")
 
    MainSection:NewDropdown("GiBEY Gun", "Gives the noob a gun", {"M9", "Remington 870", "AK-47"}, function(v)
        local A_1 = game:GetService("Workspace")["Prison_ITEMS"].giver[v].ITEMPICKUP
        local Event = game:GetService("Workspace").Remote.ItemHandler
        Event:InvokeServer(A_1)
    end)
 
    MainSection:NewDropdown("Gun Mod", "Makes the gun op", {"M9", "Remington 870", "AK-47"}, function(v)
        local module = nil
        if game:GetService("Players").LocalPlayer.Backpack:FindFirstChild(v) then
            module = require(game:GetService("Players").LocalPlayer.Backpack[v].GunStates)
        elseif game:GetService("Players").LocalPlayer.Character:FindFirstChild(v) then
            module = require(game:GetService("Players").LocalPlayer.Character[v].GunStates)
        end
        if module ~= nil then
            module["MaxAmmo"] = math.huge
            module["CurrentAmmo"] = math.huge
            module["StoredAmmo"] = math.huge
            module["FireRate"] = 0.1
            module["Spread"] = 0
            module["Range"] = math.huge
            module["Bullets"] = 1
            module["ReloadTime"] = 0.000001
            module["AutoFire"] = true
        end
    end)
 
    MainSection:NewDropdown("Shotgun mode", "Makes the gun a shotgun", {"M9", "Remington 870", "AK-47"}, function(v)
        local module = nil
        if game:GetService("Players").LocalPlayer.Backpack:FindFirstChild(v) then
            module = require(game:GetService("Players").LocalPlayer.Backpack[v].GunStates)
        elseif game:GetService("Players").LocalPlayer.Character:FindFirstChild(v) then
            module = require(game:GetService("Players").LocalPlayer.Character[v].GunStates)
        end
        if module ~= nil then
            module["MaxAmmo"] = math.huge
            module["CurrentAmmo"] = math.huge
            module["StoredAmmo"] = math.huge
            module["FireRate"] = 1
            module["Spread"] = 0
            module["Range"] = math.huge
            module["Bullets"] = 25
            module["ReloadTime"] = 0.000000000001
            module["AutoFire"] = true
        end
    end)
 
    -- PLAYER
    local Player = Window:NewTab("Player")
    local PlayerSection = Player:NewSection("Player")
 
    PlayerSection:NewSlider("Walkspeed", "Changes the walkspeed", 250, 16, function(v)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = v
    end)
 
    PlayerSection:NewSlider("Jumppower", "Changes the jumppower", 250, 50, function(v)
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = v
    end)
 
    --misc
 
    local Player = Window:NewTab("misc")
    local PlayerSection = Player:NewSection("misc")
 
    PlayerSection:NewDropdown("crash yourself", "make the gun shoot yourself", {"M9", "Remington 870", "AK-47"}, function(v)
        local module = nil
        if game:GetService("Players").LocalPlayer.Backpack:FindFirstChild(v) then
            module = require(game:GetService("Players").LocalPlayer.Backpack[v].GunStates)
        elseif game:GetService("Players").LocalPlayer.Character:FindFirstChild(v) then
            module = require(game:GetService("Players").LocalPlayer.Character[v].GunStates)
        end
        if module ~= nil then
            module["MaxAmmo"] = math.huge
            module["CurrentAmmo"] = math.huge
            module["StoredAmmo"] = math.huge
            module["FireRate"] = 0.0000000000000000001
            module["Spread"] = 0
            module["Range"] = math.huge
            module["Bullets"] = 1000000000
            module["ReloadTime"] = 0.0000000000000000000001
            module["AutoFire"] = true
            module["Damage"] = math.huge
        end
    end)
 
PlayerSection:NewToggle("god mode", "makes u god", function(state)
    if state then
        print("Toggle On")
    else
        print("Toggle Off")
    end
end)
 
PlayerSection:NewToggle("no clip", "make u go through walls", function(state)
    if state then
        print("Toggle On")
    else
        print("Toggle Off")
    end
end)
 
PlayerSection:NewToggle("fly", "make u go fly", function(state)
    if state then
       print("Toggle On")
    else
        print("Toggle Off")
    end
end)
 
 
 
 
 
PlayerSection:NewToggle("Break all doors", "no key card baby", function(state)
    if state then
       porta = game.Workspace.Doors
    :Destroy(porta)
    else
        print("Toggle Off")
    end
end)
 
 
local Main = Window:NewTab("Info")
    local MainSection = Main:NewSection("Script made by Akane#5527")
local MainSection = Main:NewSection("Join our discord for updates!")
local MainSection = Main:NewSection("This script is in beta so expect updates")
local MainSection = Main:NewSection("my youtube is https://youtu.be/9sdSXe6dqkc")
local MainSection = Main:NewSection("Lets get to Exploiting!!!")
